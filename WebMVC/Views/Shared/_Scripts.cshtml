<script>
    window.setTimeout(function () {
        $(".alert").fadeTo(500, 0).slideUp(500, function () {
            $(this).remove();
        });
    }, 5000);
    window.setTimeout(function () {
        const Toast = Swal.mixin({
            toast: true,
            position: 'top-end',
            showConfirmButton: true,
            timer: 30000,
            timerProgressBar: true,
        });
        @if (TempData["Notification"] != null)
        {
            @Html.Raw(TempData["Notification"])
        }
    });
</script>

